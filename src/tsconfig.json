{
  "compilerOptions": {
    "declaration": true,
    "lib": ["dom", "es5", "es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../lib",
    "pretty": true,
    "removeComments": false,
    "strict": true,
    "stripInternal": true,
    "target": "es5"
  }
}
